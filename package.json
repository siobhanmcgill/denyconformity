{
  "name": "denyconformity",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "venv": "source venv/bin/activate",
    "server": "python3 server/manage.py runserver",
    "build": "ng build --prod --deploy-url 'dist/'",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy:frontend": "gcloud app deploy app.yaml",
    "deploy:backend": "gcloud app deploy ./server/server-as-default.yaml",
    "deploy": "gcloud app deploy app.yaml ./server/app-server.yaml",
    "heroku-postbuild": "ng build --prod --deploy-url 'dist/'",
    "cloud_sql_proxy": "../../cloud_sql_proxy -instances=denyconformity-staging:us-central1:denyconformity=tcp:3306",
    "cloud_sql_proxy:prod": "../../cloud_sql_proxy -instances=denyconformity:us-central1:denyconformity=tcp:3306"
  },
  "private": true,
  "engines": {
    "node": "15",
    "npm": "6"
  },
  "dependencies": {
    "@angular/animations": "~11.2.3",
    "@angular/cli": "~11.2.2",
    "@angular/common": "~11.2.3",
    "@angular/compiler": "~11.2.3",
    "@angular/compiler-cli": "~11.2.3",
    "@angular/core": "~11.2.3",
    "@angular/forms": "~11.2.3",
    "@angular/platform-browser": "~11.2.3",
    "@angular/platform-browser-dynamic": "~11.2.3",
    "@angular/router": "~11.2.3",
    "rxjs": "~6.6.6",
    "showdown": "^1.9.1",
    "tslib": "^2.0.0",
    "typescript": "~4.1.5",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/language-service": "~11.2.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0"
  }
}
